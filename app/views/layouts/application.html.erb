<!DOCTYPE html>
<html>
<head>
  <%= stylesheet_link_tag    "application" %>
	<%= stylesheet_link_tag :all %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
	<%= auto_discovery_link_tag :atom, "/posts.atom" %>
</head>
<body>

	<% flash.each do |name, msg| %>
  	<%= content_tag :div, msg, :id => "flash_#{name}" %>
	<% end %>
<%= yield %>

</body>
</html>
